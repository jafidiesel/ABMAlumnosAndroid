<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monetti.proyecto.abmalumnos">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:icon="@drawable/abm"
        android:theme="@style/NewAppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".altaUsuario"/>
        <activity android:name=".bajaUsuario" />
        <activity android:name=".crashReport" />
        <activity android:name=".forceClose" />
        <activity android:name=".verUsuario"></activity>
    </application>

</manifest>